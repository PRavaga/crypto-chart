            <SolidColorBrush x:Key="TelerikDateTimePickerSelectorBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
            <SolidColorBrush x:Key="TelerikDateTimePickerSelectorForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />
