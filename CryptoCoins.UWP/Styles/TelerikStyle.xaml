            <Color x:Key="TelerikWatermarkDisabledForegroundColor">#1F000000</Color>
            <Color x:Key="TelerikForegroundColor">#FF000000</Color>
            <Color x:Key="TelerikSecondaryForegroundColor">#99000000</Color>
            <Color x:Key="TelerikSelectedColor">#FF006AC1</Color>
            <Color x:Key="TelerikHighlightedColor">#FF2086D9</Color>
            <Color x:Key="TelerikPressedBackgroundColor">#FF000000</Color>
            <Color x:Key="TelerikPressedForegroundColor">#FFFFFFFF</Color>
            <Color x:Key="TelerikDisabledColor">#66000000</Color>
            <Color x:Key="TelerikBlackColor">#FF000000</Color>
            <Color x:Key="TelerikWhiteColor">#FFFFFFFF</Color>

            <!--SelectorItem Rest State-->
