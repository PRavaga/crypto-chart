            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonPointerOverBackgroundBrush" Color="#FF999999"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonPointerOverBorderBrush" Color="#FF999999"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonPointerOverForegroundBrush" Color="{ThemeResource TelerikBlackColor}"/>

            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonPressedBackgroundBrush" Color="{ThemeResource TelerikPressedBackgroundColor}"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonPressedBorderBrush" Color="{ThemeResource TelerikPressedBackgroundColor}"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonPressedForegroundBrush" Color="{ThemeResource TelerikPressedForegroundColor}"/>

            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedBorderBrush" Color="{ThemeResource TelerikWhiteColor}"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedForegroundBrush" Color="{ThemeResource TelerikSelectedColor}"/>

            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedPointerOverBackgroundBrush" Color="#FF999999"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedPointerOverBorderBrush" Color="{ThemeResource TelerikForegroundColor}"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedPointerOverForegroundBrush" Color="{ThemeResource TelerikSelectedColor}"/>

            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedPressedBackgroundBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedPressedBorderBrush" Color="{ThemeResource TelerikForegroundColor}"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutTextCaseButtonCheckedPressedForegroundBrush" Color="{ThemeResource TelerikSelectedColor}"/>

            <!-- Column chooser -->
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxForegroundBrush" Color="{ThemeResource TelerikWhiteColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxPointerOverBackgroundBrush" Color="{ThemeResource TelerikHighlightedColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxPointerOverForegroundBrush" Color="{ThemeResource TelerikWhiteColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxPressedBackgroundBrush" Color="{ThemeResource TelerikPressedBackgroundColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxPressedForegroundBrush" Color="{ThemeResource TelerikPressedForegroundColor}"/>

            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxCheckedBackgroundBrush" Color="{ThemeResource TelerikSelectedColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxCheckedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxCheckedPointerOverBackgroundBrush" Color="{ThemeResource TelerikHighlightedColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxCheckedPointerOverForegroundBrush" Color="{ThemeResource TelerikWhiteColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxCheckedPressedBackgroundBrush" Color="{ThemeResource TelerikPressedBackgroundColor}"/>
            <SolidColorBrush x:Key="TelerikGridColumnFlyoutColumnHeaderCheckBoxCheckedPressedForegroundBrush" Color="{ThemeResource TelerikPressedForegroundColor}"/>

            <BitmapImage x:Key="TelerikGridReorderHandle" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/Reorder/ic_reorder_light.png"></BitmapImage>
            <BitmapImage x:Key="TelerikGridReorderHandleWhite" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/Reorder/ic_reorder_dark.png"></BitmapImage>
            <BitmapImage x:Key="TelerikGridColumnChooserOpen" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/Reorder/column_chooser_btn.png"></BitmapImage>
            <BitmapImage x:Key="TelerikGridColumnChooserOpenMouseOver" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/Reorder/column_chooser_btn_blue.png"></BitmapImage>
            <BitmapImage x:Key="TelerikGridColumnChooserClose" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/Reorder/close_btn.png"></BitmapImage>
            <BitmapImage x:Key="TelerikGridColumnChooserCloseMouseOver" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/Reorder/close_btn_blue.png"></BitmapImage>

            <!--FilterFlyoutIcons-->
            <BitmapImage x:Key="TelerikGridFilterFlyoutHeaderIcon" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/FilterFlyout/ic_filter_header_light.png"/>
            <BitmapImage x:Key="TelerikGridFilterFlyoutExpanderIcon" UriSource="ms-appx:///Telerik.UI.Xaml.Grid.UWP/Assets/FilterFlyout/ic_arrow_down.png"/>






            <Color x:Key="TelerikSegmentedControlDisabledColor">#FFD9D9D9</Color>

            <SolidColorBrush x:Key="TelerikSegmentedControlBackgroundThemeBrush" Color="Transparent" />
            <SolidColorBrush x:Key="TelerikSegmentedControlBorderThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
            <SolidColorBrush x:Key="TelerikSegmentedControlSeparatorThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />

            <SolidColorBrush x:Key="TelerikSegmentedControlDisabledBackgroundThemeBrush" Color="Transparent" />
            <SolidColorBrush x:Key="TelerikSegmentedControlDisabledBorderThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
            <SolidColorBrush x:Key="TelerikSegmentedControlDisabledSeparatorThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />

            <!--segment normal-->

            <SolidColorBrush x:Key="TelerikSegmentBackgroundThemeBrush" Color="Transparent" />
            <SolidColorBrush x:Key="TelerikSegmentContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
            <SolidColorBrush x:Key="TelerikSegmentBorderThemeBrush" Color="Transparent" />

            <!--segment pointer over-->

            <SolidColorBrush x:Key="TelerikSegmentPointerOverBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" Opacity="0.4" />
            <SolidColorBrush x:Key="TelerikSegmentPointerOverContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
            <SolidColorBrush x:Key="TelerikSegmentPointerOverBorderThemeBrush" Color="Transparent" />

            <!--segment pressed-->

            <SolidColorBrush x:Key="TelerikSegmentPressedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
            <SolidColorBrush x:Key="TelerikSegmentPressedContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
            <SolidColorBrush x:Key="TelerikSegmentPressedBorderThemeBrush" Color="Transparent" />

            <!--segment disabled-->

            <SolidColorBrush x:Key="TelerikSegmentDisabledBackgroundThemeBrush" Color="Transparent" />
            <SolidColorBrush x:Key="TelerikSegmentDisabledContentForegroundThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
            <SolidColorBrush x:Key="TelerikSegmentDisabledBorderThemeBrush" Color="Transparent" />

            <!--segment selected-->

            <SolidColorBrush x:Key="TelerikSegmentSelectedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedBorderThemeBrush" Color="Transparent" />

            <!--segment selected pointer over-->

            <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" Opacity="0.7" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverBorderThemeBrush" Color="Transparent" />

            <!--segment selected pressed-->

            <SolidColorBrush x:Key="TelerikSegmentSelectedPressedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedPressedContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedPressedBorderThemeBrush" Color="Transparent" />

            <!--segment selected disabled-->

            <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledBackgroundThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledContentForegroundThemeBrush" Color="{ThemeResource TelerikWhiteColor}" />
            <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledBorderThemeBrush" Color="Transparent" />

            <!--#endregion-->

            <!--#region AutoCompleteBox Brushes-->

            <!--Inline-->

            <SolidColorBrush x:Key="TelerikAutoCompleteBoxForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />

            <SolidColorBrush x:Key="TelerikAutoCompleteBoxBackgroundBrush" Color="#CCFFFFFF" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxBorderBrush" Color="#45000000" />

            <SolidColorBrush x:Key="TelerikAutoCompleteBoxHeaderForegroundBrush" Color="{ThemeResource TelerikSecondaryForegroundColor}" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxWatermarkForegroundBrush" Color="{ThemeResource TelerikWatermarkForegroundColor}" />

            <!--PointerOver-->
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxPointerOverBackgroundBrush" Color="#DEFFFFFF" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxPointerOverBorderBrush" Color="#70000000" />

            <!--Focused-->
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedForegroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedBackgroundBrush" Color="{ThemeResource TelerikWhiteColor}" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedBorderBrush" Color="#A3000000" />

            <!--Disabled-->
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledForegroundBrush" Color="{ThemeResource TelerikDisabledColor}" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledBackgroundBrush" Color="#66CACACA" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledBorderBrush" Color="{ThemeResource TelerikDisabledColor}" />
            <SolidColorBrush x:Key="TelerikAutoCompleteBoxWatermarkDisabledForegroundBrush" Color="{ThemeResource TelerikWatermarkDisabledForegroundColor}" />

