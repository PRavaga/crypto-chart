            <SolidColorBrush x:Key="TelerikGridFilterFlyoutHeaderForegroundBrush" Color="#FF454545"/>
            <SolidColorBrush x:Key="TelerikGridFilterFlyoutHeaderBackgroundBrush" Color="#FFCCCCCC"/>
