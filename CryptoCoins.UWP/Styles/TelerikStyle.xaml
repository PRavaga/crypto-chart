            <SolidColorBrush x:Key="TelerikDateTimePickerBorderBrush" Color="#45000000" />
            <SolidColorBrush x:Key="TelerikDateTimePickerArrowForegroundBrush" Color="{ThemeResource TelerikBlackColor}" />

