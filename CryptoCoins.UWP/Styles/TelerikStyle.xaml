            <!--CellFlyoutControl-->
            <SolidColorBrush x:Key="TelerikGridCellFlyoutControlBackgroundBrush" Color="#EEEEEE" />
            <SolidColorBrush x:Key="TelerikGridCellFlyoutControlBorderBrush" Color="#999999" />
            <SolidColorBrush x:Key="TelerikGridCellFlyoutControlOuterBorderBrush" Color="#FF838282" />
            <x:Double x:Key="TelerikGridCellFlyoutControlOuterBorderOpacity">0.3</x:Double>

